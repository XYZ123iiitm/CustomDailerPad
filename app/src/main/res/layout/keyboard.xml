			android:layout_weight="1"
			android:background="@drawable/borders"
			android:gravity="center"
			android:padding="5dp"
			android:layout_marginRight="5dp"
			/>

	</LinearLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
