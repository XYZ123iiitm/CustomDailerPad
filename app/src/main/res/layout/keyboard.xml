			<ImageButton
				android:background="@drawable/bg_button"
				android:layout_weight="7"
				android:id="@+id/Buttoncall"
				android:onClick="buttoncall"
				style="@style/DialerButton"
				android:src="@drawable/button_call"/>

			<ImageButton
