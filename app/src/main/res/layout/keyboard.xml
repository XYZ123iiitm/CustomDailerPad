	android:id="@+id/hebrwKeyboardView"
	android:layout_width="fill_parent"
	android:layout_alignParentBottom="true"
	android:layout_below="@+id/xsubLayout"
	android:orientation="vertical"
	android:background="#1E90FF"
	android:visibility="visible"
	android:layout_weight="6"
	android:layout_gravity="bottom"
	android:layout_height="wrap_content">


	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_weight="1"
		android:layout_marginBottom="5dp">

		<EditText
			android:id="@+id/editText_dialpad"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="3"
			android:clickable="false"
			android:cursorVisible="false"
			android:focusable="false"
			android:hint="@string/Search_in_contact"
			android:textColorHint="#F0F8FF"
			android:textColor="#ffffff"
			android:inputType="phone|number"
			android:textSize="25sp" />

		<ImageButton
			android:id="@+id/ButtonDelete1"
			android:onClick="buttoncancel"
			android:background="@drawable/bg_button"
			android:src="@drawable/button_backspace"
			android:layout_marginLeft="0dp"
			android:layout_marginRight="0dp"
			android:layout_width="0px"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:contentDescription="@string/erase"
			android:longClickable="true" />



	</LinearLayout>


	<LinearLayout
